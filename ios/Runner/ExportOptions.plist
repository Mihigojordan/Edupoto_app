<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Store distribution method -->
    <key>method</key>
    <string>app-store</string>

    <!-- Embed Swift libraries -->
    <key>embedSwiftStandardLibraries</key>
    <true/>

    <!-- Do not strip Swift symbols -->
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- Disable bitcode compilation (Flutter apps usually do this) -->
    <key>compileBitcode</key>
    <false/>

    <!-- Automatic signing -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Include dSYM for crash reports -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Include app thinning options -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
</dict>
</plist>
